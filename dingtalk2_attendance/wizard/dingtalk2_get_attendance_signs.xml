<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_dingtalk2_get_attendance_signs_form" model="ir.ui.view">
        <field name="name">dingtalk2.get.attendance.signs.form</field>
        <field name="model">dingtalk2.get.attendance.signs</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="start_date"/>
                    </group>
                    <group>
                        <field name="end_date"/>
                    </group>
                </group>
                <footer>
                    <button string="确认获取" name="on_get" type="object" class="oe_highlight"/>
                    <button string="取消" class="btn btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>