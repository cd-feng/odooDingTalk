		<record id="ali_dindin_system_conf_token" model="ali.dindin.system.conf">
			<field name="name">阿里钉钉-Token值</field>
			<field name="key">token</field>
			<field name="value">0000</field>
		</record>

        <record id="ali_dindin_system_conf_gettoken" model="ali.dindin.system.conf">
			<field name="name">获取Token值URL</field>
