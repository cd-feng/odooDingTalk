<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- 主权限 -->
    <record model="ir.module.category" id="main_groups">
        <field name="name">钉钉权限</field>
        <field name="visible" eval="0"/>
    </record>
    <record id="manage_groups" model="res.groups">
        <field name="name">管理员</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user')), (4, ref('dingtalk_mini.dingtalk_mini_roster_group')),
        (4, ref('dingtalk_mini.dingtalk_mini_approval_group')), (4, ref('dingtalk_mini.dingtalk_mini_attendance_group')),
        (4, ref('dingtalk_mini.dingtalk_mini_message_group')), (4, ref('dingtalk_mini.dingtalk_miroapp_group'))]"/>
        <field name="category_id" ref="dingtalk_mini.main_groups"/>
        <field name="users" eval="[(4, ref('base.user_admin'))]"/>
    </record>

</odoo>
