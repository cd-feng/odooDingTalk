<?xml version="1.0" encoding="UTF-8"?>
<template id="template" xml:space="preserve">

    <t t-name="PullDinDinAttendanceList">
        <div class="report-properties">
            <div class="col-md-10">
                <div class="form-group">
                    <label for="startDate">开始日期</label>
                    <em>必填项!</em>
                    <input type="date" required="required" class="form-control" name="startDate" id="startDate"
                           value="2019-03-13"/>
                </div>
                <div class="form-group">
                    <label for="endDate">结束日期</label>
                    <em>必填项!</em>
                    <input type="date" required="required" class="form-control" name="endDate" id="endDate"
                           value="2019-03-20"/>
                </div>
            </div>
            <div class="col-md-10">
                <div class="form-group">
                    <label for="slipNo">员工名称</label>
                    <em>不填则默认全部员工</em>
                    <input type="text" required="required" class="form-control" name="username" id="username"/>
                </div>
            </div>
            <div class="alert alert-info">
                注意:开始和结束日期不能大于7天!!!
            </div>
        </div>
	</t>

</template>
