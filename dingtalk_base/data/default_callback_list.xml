            <field name="value">label_user_change</field>
            <field name="value_type">00</field>
            <field name="call_back_url">web/dingtalk/callback/do</field>
        </record>
        <record id="dingtalk_callback_list_label_conf_add" model="dingtalk.callback.list">
            <field name="name">增加角色或者角色组</field>
            <field name="value">label_conf_add</field>
            <field name="value_type">00</field>
            <field name="call_back_url">web/dingtalk/callback/do</field>
