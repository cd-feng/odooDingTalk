        <record id="dingtalk_callback_list_org_remove" model="dingtalk.callback.list">
            <field name="name">企业被解散</field>
            <field name="value">org_remove</field>
            <field name="value_type">00</field>
            <field name="call_back_url">web/dingtalk/callback/do</field>
        </record>
        <record id="dingtalk_callback_list_user_org_change" model="dingtalk.callback.list">
